#|
(defio
  input  (device-file "/dev/input/by-id/some-keyboard")
  output (uinput-sink "my-keyboard-name")
  init   "/run/current-system/sw/bin/sleep 1 && /run/current-system/sw/bin/setxkbmap -option compose:ralt"
)
|#

#|
;; Define the input-layout coming from the keyboard
(defsrc
  q    w    f    p    g              j    l    u    y    ;
  a    r    s    t    d              h    n    e    i    o
  z    x    c    v    b              k    m    ,    .    /
  tab  lmet lalt lsft lctl ins  esc  spc  rmet -    bspc ret
)
|#

#|
(defalias
  hello (layer-toggle test)
  bar   P
)
|#

;; Define the input-layout coming from
(deflayer colemak
  _    _X   \_   P    c              j    l    u    y    ;
  a    r    s    t    d              h    n    e    i    o
  z    x    c    v    b              k    m    ,    .    /
  tab  lmet lalt lsft lctl ins  esc  spc  rmet -    bspc ret
)
